#CLI command to show version
<?xml version="1.0"?>
<nf:rpc xmlns:nf="urn:ietf:params:xml:ns:netconf:base:1.0" xmlns="http://www.cisco.com/nxos:7.3.1.D1.1.:sysmgrcli" message-id="1">
  <nf:get>
    <nf:filter type="subtree">
      <show>
        <version/>
      </show>
    </nf:filter>
  </nf:get>
</nf:rpc>
]]>]]>
